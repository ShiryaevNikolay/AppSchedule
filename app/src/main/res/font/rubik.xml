<?xml version="1.0" encoding="utf-8"?>
<font-family
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <font
        android:fontStyle="normal"
        android:fontWeight="900"
        android:font="@font/rubik_bold"
        app:fontStyle="normal"
        app:fontWeight="900"
        app:font="@font/rubik_bold"/>
    <font
        android:fontStyle="normal"
        android:fontWeight="700"
        android:font="@font/rubik_medium"
        app:fontStyle="normal"
        app:fontWeight="700"
        app:font="@font/rubik_medium"/>
    <font
        android:fontStyle="normal"
        android:fontWeight="500"
        android:font="@font/rubik_medium"
        app:fontStyle="normal"
        app:fontWeight="500"
        app:font="@font/rubik_medium"/>
    <font
        android:fontStyle="normal"
        android:fontWeight="400"
        android:font="@font/rubik_regular"
        app:fontStyle="normal"
        app:fontWeight="400"
        app:font="@font/rubik_regular"/>
    <font
        android:fontStyle="normal"
        android:fontWeight="300"
        android:font="@font/rubik_light"
        app:fontStyle="normal"
        app:fontWeight="300"
        app:font="@font/rubik_light"/>

    <font
        android:fontStyle="italic"
        android:fontWeight="900"
        android:font="@font/rubik_black_italic"
        app:fontStyle="italic"
        app:fontWeight="900"
        app:font="@font/rubik_black_italic"/>
    <font
        android:fontStyle="italic"
        android:fontWeight="700"
        android:font="@font/rubik_bold_italic"
        app:fontStyle="italic"
        app:fontWeight="700"
        app:font="@font/rubik_bold_italic"/>
    <font
        android:fontStyle="italic"
        android:fontWeight="500"
        android:font="@font/rubik_medium_italic"
        app:fontStyle="italic"
        app:fontWeight="500"
        app:font="@font/rubik_medium_italic"/>
    <font
        android:fontStyle="italic"
        android:fontWeight="400"
        android:font="@font/rubik_italic"
        app:fontStyle="italic"
        app:fontWeight="400"
        app:font="@font/rubik_italic"/>
    <font
        android:fontStyle="italic"
        android:fontWeight="300"
        android:font="@font/rubik_light_italic"
        app:fontStyle="italic"
        app:fontWeight="300"
        app:font="@font/rubik_light_italic"/>
</font-family>